<div id="<%= dom_id patient %>">
  <p>
    <strong>First name:</strong>
    <%= patient.first_name %>
  </p>

  <% patient.pre_existing_conditions.map do |pre_existing_condition| %>
    <p>
      <strong>Diabetes</strong>
      <%= pre_existing_condition.diabetes %>
    </p>
    <p>
      <strong>asthma</strong>
      <%= pre_existing_condition.asthma %>
    </p>
    <p>
    <strong>hypertension</strong>
    <%= pre_existing_condition.hypertension %>
  </p>
  <% end %>

  <p>
    <strong>Last name:</strong>
    <%= patient.last_name %>
  </p>

  <p>
    <strong>National:</strong>
    <%= patient.national_id %>
  </p>

  <p>
    <strong>Image:</strong>
    <%= patient.image %>
  </p>

  <p>
    <strong>Telephone:</strong>
    <%= patient.telephone %>
  </p>

  <p>
    <strong>Gender:</strong>
    <%= patient.gender %>
  </p>

  <p>
    <strong>Doctor:</strong>
    <%= patient.doctor_id %>
  </p>

</div>
